{
  "name": "my-project",
  "version": "1.0.0",
  "scripts": {
    "sass:watch": "sass --watch scss:css --style expanded",
    "sass:build": "sass scss:css --style compressed --no-source-map",
    "sass:dev": "sass --watch scss:css --style expanded --source-map",
    "build": "npm run sass:build",
    "dev": "npm run sass:dev",
    "start": "npm run dev"
  },
  "devDependencies": {
    "sass": "^1.75.0"
  }
}